<table class="table table-bordered table-hover">
  <thead>
  <tr>
    <th>ID</th>
    <th>Object Type</th>
    <th>Object ID</th>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Modify</th>
  </tr>
  </thead>

  <tbody>
  <% events.each do |event| %>
    <tr>
      <td><%= event.id %></td>
      <td><%= event.object_type %></td>
      <td><%= event.object_id %></td>
      <td><%= event.created_at %></td>
      <td><%= event.updated_at %></td>
      <td><%= link_to 'Show History', show_history_path(event.id) %></td>
    </tr>
  <% end %>
  </tbody>
</table>